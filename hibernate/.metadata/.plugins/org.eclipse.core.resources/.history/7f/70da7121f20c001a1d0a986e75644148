package com.ustglobal.jpawithhibernat.onetoone;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Data;

@Data
@Entity
@Table
public class Votercard {
	@Id
	@Column
	private int vid;
	@Column
	private String name;
	



}
